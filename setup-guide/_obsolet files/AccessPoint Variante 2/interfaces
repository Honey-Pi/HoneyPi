# AccessPoint:
auto wlan0

iface wlan0 inet dhcp
    wpa-conf /etc/wpa_supplicant/wpa_supplicant.conf
    wireless-power off



iface wlan0 inet static
    address 192.168.255.1
    netmask 255.255.255.0
    wireless-mode Master
    wireless-power off
	
# Surfstick:
#allow-hotplug wwan0
#iface wwan0 inet dhcp
